{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["prettier", "jest"],
  "rules": {
    "import/no-unresolved": [2, { "caseSensitive": false }],
    "object-curly-newline": ["error", {"ObjectExpression": "always", "ImportDeclaration": "never"}],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "endOfLine": "auto"
      }
    ],"comma-dangle": ["error", "only-multiline"]
  }       
}
